{
  "name": "supereffective-website",
  "version": "3.10.0",
  "private": true,
  "repository": "git@github.com:supeffective/website.git",
  "license": "MIT",
  "author": "Javi Aguilar https://itsjavi.com",
  "scripts": {
    "build": "next build && next-sitemap",
    "build-vercel": "pnpm typecheck && pnpm db-migrate && pnpm build",
    "db-migrate": "pnpm prisma migrate deploy",
    "dev": "rm -rf ./.next && PORT=3001 next dev",
    "format": "sort-package-json && prettier --ignore-path '.prettierignore' --write ./",
    "lint": "pnpm typecheck && prettier --ignore-path '.prettierignore' --check ./",
    "lint-fix": "pnpm format",
    "prepare": "pnpm prisma generate && sort-package-json",
    "qa": "pnpm quality-checks",
    "quality-checks": "pnpm quality-checks-nobuild && turbo build",
    "quality-checks-nobuild": "rm -rf .next && pnpm prisma generate && turbo lint && pnpm test",
    "start": "next start",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "5.12.1",
    "@supeffective/dataset": "^3.0.2",
    "@supeffective/icons": "^0.8.6",
    "clsx": "^2.1.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.368.0",
    "nanoid": "^5.0.7",
    "next": "14.2.0",
    "next-auth": "^4.24.7",
    "next-axiom": "1.1.1",
    "next-sitemap": "^4.2.3",
    "prisma": "^5.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-modal": "^3.16.1",
    "resend": "^3.2.0",
    "sass": "^1.75.0",
    "sharp": "0.33.3",
    "sort-package-json": "^2.10.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@swc/core": "^1.4.13",
    "@swc/jest": "^0.2.36",
    "@types/bun": "^1.0.12",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18.2.25",
    "@types/react-modal": "^3.16.3",
    "execa": "^8.0.1",
    "glob": "^10.3.12",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "pwa-asset-generator": "^6.3.1",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "nodemailer": "^6.9.12"
  },
  "packageManager": "pnpm@8.15.5",
  "engines": {
    "node": ">=18.10.0 <21"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "cpu": [
    "arm64",
    "x64"
  ],
  "prisma": {
    "schema": "src/prisma/schema.prisma"
  }
}
